<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jwl.sns.post.comment.dao.CommentDAO">
	
	<insert id="insertComment" parameterType="map">
		INSERT INTO
			`comment`
		(
			`userId`
			, `postId`
			, `content`
			, `createdAt`
			, `updatedAt`
		)
		VALUE
		(
			#{userId}
			, #{postId}
			, #{content}
			, now()
			, now()
		)
	</insert>

</mapper>